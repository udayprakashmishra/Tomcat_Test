pipeline {
  agent any
  stages {
    
      stage('Build Application') {
            steps {
            echo "Started"
               sh 'mvn -f pom.xml clean package'
                echo "Ended"
            }
            post {
                success {
                    echo "Now Archiving the Artifacts...."
                    archiveArtifacts artifacts: '**/*.war'
                }
            }
        }
      
  }
}

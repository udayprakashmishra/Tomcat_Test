pipeline {
  agent any
  stages {
    
      stage('Build Application') {
            steps {
            echo "Started"
               bat 'mvn -f pom.xml clean package'
                echo "Ended"
            }
            post {
                success {
                    echo "Now Archiving the Artifacts...."
                    archiveArtifacts artifacts: '**/*.war'
                }
            }
        }
        
        stage('Deploy in Dev') {
        build job: 'DSLMavenCodePipelinePackage_Deploy'
        }
      
  }
}
